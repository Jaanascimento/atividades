
function triângulo (l1, l2, l3) {
    if (l1 == l2 && l1 == l3) {
        console.log("O triângulo é equilátero");
    } else if (l1 == l2 || l2 == l3 || l1 == l3) {
        console.log("O triângulo é isósceles");
    } else {
        console.log("O triângulo é escaleno");
    }
}

triângulo(3, 3, 3)
triângulo(4, 3, 3)
triângulo(1, 2, 3)
